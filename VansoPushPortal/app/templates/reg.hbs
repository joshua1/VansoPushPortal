 <div class="row-fluid">
                        <div class="span12">
                            <div class="box alternate">
                                <div class="box-title">
                                    Login
                                </div>
                                <div class="box-content nopadding">
                                    <form name="exampleform" action="#" method="get" class="form-horizontal">
                                        <div class="control-group">
                                            <label class="control-label">Email</label>
                                            <div class="controls">
                                                {{view Ember.Textfield valueBinding="email"}}
                                                <span class="help-inline">Enter your email address</span>
                                            </div>
                                        </div>
                                        <div class="control-group">
                                            <label class="control-label">Password</label>
                                            <div class="controls">
                                               {{view Ember.Textfield type="password" valueBinding="password"}}
                                                <span class="help-inline">Your password</span>
                                            </div>
                                        </div>
                                         <div class="form-actions">
                                         <a class="btn" {{action authenticate target="controller"}}>Log in</a>
                                         <a class="btn" {{action register}}>click me</a>
                                            <input type="submit" class="btn btn-primary" value="Login" />
                                               <button class="btn" >Register</button>
                                            </div>
                                     </form>
                                 </div>
                             </div>
                        </div>
 </div>